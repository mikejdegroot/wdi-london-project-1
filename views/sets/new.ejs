<h1>set create</h1>
<!-- form for adding new entry to db-->
<form class="col-lg-4 submit form" name="_method" method="POST" action="/sets">
  <div class="form-group">
    <label>Set Name</label>
    <input type="text" class="form-control" name="name" id="nameInput" placeholder="Set Name">
  </div>
  <div class="form-group">
    <label>Date</label>
    <input type="text" class="form-control" name="date"id="dateInput" placeholder="DD/MM/YYYY">
  </div>
  <button type="submit" class="btn btn-primary btn-block">Submit</button>
</form>
